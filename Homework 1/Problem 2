public class Main {
    public static void main(String[] args) {
        Duck duck = new Duck(2, "Female");
        duck.swim();
        duck.quack();

        Fish fish = new Fish(1, "Male", 3, true);
        fish.swim();

        Zebra zebra = new Zebra(4, "Female", true);
        zebra.run();
    }
}

class Animal {
    int age;
    String gender;

    Animal(int age, String gender) {
        this.age = age;
        this.gender = gender;
    }

    boolean isMammal() {
        return this instanceof Zebra;
    }

    void mate() {
        System.out.println(getClass().getSimpleName() + " is mating.");
    }
}

class Duck extends Animal {
    String beakColor = "yellow";

    Duck(int age, String gender) {
        super(age, gender);
    }

    void swim() {
        System.out.println("Duck is swimming.");
    }

    void quack() {
        System.out.println("Duck is quacking.");
    }
}

class Fish extends Animal {
    int sizeInFt;
    boolean canEat;

    Fish(int age, String gender, int sizeInFt, boolean canEat) {
        super(age, gender);
        this.sizeInFt = sizeInFt;
        this.canEat = canEat;
    }

    void swim() {
        System.out.println("Fish is swimming.");
    }
}

class Zebra extends Animal {
    boolean is_wild;

    Zebra(int age, String gender, boolean is_wild) {
        super(age, gender);
        this.is_wild = is_wild;
    }

    void run() {
        System.out.println("Zebra is running.");
    }
}
